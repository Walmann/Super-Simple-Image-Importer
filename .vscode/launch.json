    "version": "0.2.0",
    "configurations": [
        {
            "name": "All Scripts Current File",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "test.Py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/bin/test.py",
            "cwd": "${workspaceFolder}/src",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "app.Py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/app.py",
            "cwd": "${workspaceFolder}/src",
            "console": "integratedTerminal",
            "justMyCode": true,
            "qt": "auto",
            "subProcess": true
        },
        {
            "name": "Build EXE",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/Installer/create_new_release.py",
            "args": ["-exe"],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Build Fast EXE",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/Installer/create_new_release.py",
            "args": ["-exeFast"],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Build Setup",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/Installer/create_new_release.py",
            "args": ["-installer"],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Build EXE and Setup",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/Installer/create_new_release.py",
            "args": ["-exe", "-installer"],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Build and publish",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/Installer/create_new_release.py",
            "args": ["-exe", "-installer", "-publish"],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Publish Only",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/Installer/create_new_release.py",
            "args": ["-publish"],
            "console": "integratedTerminal",
            "justMyCode": true
        },
