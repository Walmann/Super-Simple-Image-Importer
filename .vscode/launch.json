    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Import.Py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/Importer.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "BUILD_EXE Setup.py ",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/setup.py",
            "args": [
                "build_exe"
            ],
            "console": "integratedTerminal"
        },
