            "name": "app.Py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/bin/app.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "qt": "auto",
            "subProcess": true
        },
        {
            "name": "Create New Release.Py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/Installer/create_new_release.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
